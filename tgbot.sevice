[Unit]
Description=Telegram Bot
After=network.target

[Service]
User=alexadmin
Group=alexadmin

WorkingDirectory=/home/alexadmin/alexproject/
Environment="PYTHONPATH=/home/alexadmin/alexproject/"
ExecStart=/home/alexadmin/alexproject/.venv/bin/python /home/alexadmin/alexproject/main.py

[Install]
WantedBy=multi-user.target
